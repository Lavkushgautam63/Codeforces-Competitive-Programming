#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin >> n;

    vector<long long> b(n), a(n);
    for (int i = 0; i < n; i++) cin >> b[i];

    long long mx = 0;

    for (int i = 0; i < n; i++) {
        a[i] = b[i] + mx;
        mx = max(mx, a[i]);
    }

    for (auto v : a) cout << v << " ";
}
