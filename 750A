#include <bits/stdc++.h>
using namespace std;

int main() {
    int n, k;
    cin >> n >> k;

    int available = 240 - k;
    int used = 0;
    int solved = 0;

    for (int i = 1; i <= n; i++) {
        used += 5 * i;
        if (used > available) break;
        solved++;
    }

    cout << solved;
    return 0;
}
