#include <bits/stdc++.h>
using namespace std;

int main() {
    int q;
    cin >> q;
    while (q--) {
        long long n;
        cin >> n;

        long long a = 0, b = 0, c = 0;
        while (n % 2 == 0) n /= 2, a++;
        while (n % 3 == 0) n /= 3, b++;
        while (n % 5 == 0) n /= 5, c++;

        if (n != 1) {
            cout << -1 << '\n';
        } else {
            cout << a + 2*b + 3*c << '\n';
        }
    }
    return 0;
}
