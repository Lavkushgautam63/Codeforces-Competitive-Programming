#include <iostream>
using namespace std;

int main() {
    int t;
    cin >> t;
    
    while(t--) {
        int a1, b1, a2, b2;
        cin >> a1 >> b1;
        cin >> a2 >> b2;
        
        bool possible = false;
        
        int r1[2][2] = {{a1, b1}, {b1, a1}};
        int r2[2][2] = {{a2, b2}, {b2, a2}};
        
        for(int i = 0; i < 2; i++) {
            for(int j = 0; j < 2; j++) {
                int h1 = r1[i][0], w1 = r1[i][1];
                int h2 = r2[j][0], w2 = r2[j][1];
                
                if(h1 == h2 && w1 + w2 == h1)
                    possible = true;
            }
        }
        
        if(possible) cout << "YES\n";
        else cout << "NO\n";
    }
}
