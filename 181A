#include <bits/stdc++.h>
using namespace std;

int main() {
    int n, m;
    cin >> n >> m;

    vector<int> r, c;
    for (int i = 1; i <= n; i++) {
        string s;
        cin >> s;
        for (int j = 1; j <= m; j++) {
            if (s[j-1] == '*') {
                r.push_back(i);
                c.push_back(j);
            }
        }
    }

    int row, col;
    if (r[0] == r[1]) row = r[2];
    else if (r[0] == r[2]) row = r[1];
    else row = r[0];

    if (c[0] == c[1]) col = c[2];
    else if (c[0] == c[2]) col = c[1];
    else col = c[0];

    cout << row << " " << col;
    return 0;
}
