#include <bits/stdc++.h>
using namespace std;

int main() {
    int n, c;
    cin >> n >> c;

    vector<int> p(n), t(n);
    for (int i = 0; i < n; i++) cin >> p[i];
    for (int i = 0; i < n; i++) cin >> t[i];

    long long limak = 0, radewoosh = 0;
    long long time = 0;

    for (int i = 0; i < n; i++) {
        time += t[i];
        limak += max(0LL, p[i] - c * time);
    }

    time = 0;
    for (int i = n - 1; i >= 0; i--) {
        time += t[i];
        radewoosh += max(0LL, p[i] - c * time);
    }

    if (limak > radewoosh) cout << "Limak";
    else if (limak < radewoosh) cout << "Radewoosh";
    else cout << "Tie";

    return 0;
}
