#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin >> n;
    vector<string> bus(n);
    for (int i = 0; i < n; i++) cin >> bus[i];

    bool found = false;

    for (int i = 0; i < n && !found; i++) {
        if (bus[i][0] == 'O' && bus[i][1] == 'O') {
            bus[i][0] = bus[i][1] = '+';
            found = true;
        } else if (bus[i][3] == 'O' && bus[i][4] == 'O') {
            bus[i][3] = bus[i][4] = '+';
            found = true;
        }
    }

    if (found) {
        cout << "YES\n";
        for (auto &row : bus) cout << row << "\n";
    } else {
        cout << "NO\n";
    }

    return 0;
}
