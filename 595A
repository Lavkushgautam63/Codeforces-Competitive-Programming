#include <bits/stdc++.h>
using namespace std;

int main() {
    int n, m;
    cin >> n >> m;
    int count = 0;

    for (int i = 0; i < n; i++) {
        vector<int> windows(2 * m);
        for (int j = 0; j < 2 * m; j++) cin >> windows[j];

        for (int j = 0; j < m; j++) {
            if (windows[2 * j] == 1 || windows[2 * j + 1] == 1)
                count++;
        }
    }

    cout << count << endl;
    return 0;
}
