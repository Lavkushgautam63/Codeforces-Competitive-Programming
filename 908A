#include <bits/stdc++.h>
using namespace std;

int main() {
    string s;
    cin >> s;

    set<char> vowels = {'a','e','i','o','u'};
    set<char> odd = {'1','3','5','7','9'};

    int flips = 0;
    for (char c : s) {
        if (vowels.count(c) || odd.count(c)) {
            flips++;
        }
    }

    cout << flips << endl;
    return 0;
}
