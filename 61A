#include <iostream>
using namespace std;

int main() {
    string a, b;
    cin >> a >> b;

    int n = a.length();
    string result = "";

    for (int i = 0; i < n; i++) {
        if (a[i] == b[i])
            result += '0';
        else
            result += '1';
    }

    cout << result;
    return 0;
}
